<div class="page-header">
  <h1>
    Nuevo servicio

  </h1>
</div><!-- /.page-header -->

<div class="row">


  <div class="col-xs-12">
    <!-- PAGE CONTENT BEGINS -->
    <%= form_for @service, :url => '/services', :html => {:class => "form-horizontal", :role => "form"} do |f| %>

        <%= render :partial => 'form', :service => @service, :locals => {:f => f, :form_validation => 'data'|| 'no-data'} %>



        <div class="form-group">
          <div class="col-sm-offset-3 col-sm-9">
            <a href="/services">
              <button type="button" class="btn btn-default">Cancelar</button>
            </a>
            <button type="submit" class="btn btn-success">Guardar</button>
          </div>
        </div>

    <% end %>
    <!-- PAGE CONTENT ENDS -->
  </div>
  <!-- /.col -->
</div><!-- /.row